{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "Dual Import Manifest",
  "type": "object",
  "required": ["module", "imports"],
  "properties": {
    "module": { "type": "string", "description": "Root module being normalized" },
    "imports": {
      "type": "array",
      "items": {
        "type": "object",
        "required": ["original", "normalized"],
        "properties": {
          "original": { "type": "string" },
          "normalized": { "type": "string" },
          "reason": { "type": "string" },
          "status": { "type": "string", "enum": ["kept", "rewritten", "deprecated"] }
        }
      }
    },
    "generated_at": { "type": "string", "format": "date-time" }
  }
}
